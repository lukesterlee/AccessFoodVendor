<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context="take2.c4q.nyc.accessfoodvendor.RegisterActivity"
    android:orientation="vertical"
    android:background="#03a9f4"
    app:popupTheme="@style/ThemeOverlay.AppCompat.Light">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:layout_centerHorizontal="true"
        android:weightSum="1"
        android:padding="10dp">

        <ImageView
            android:layout_width="118dp"
            android:layout_height="60dp"
            android:id="@+id/icon"
            android:src="@drawable/whitelogo"
            android:layout_marginTop="10dp"
            />

        <TextView
            android:layout_marginTop="25dp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="Hours"
            android:textColor="@color/white"
            android:textSize="20sp"
            android:textStyle="bold"
            android:layout_weight="0.19"
            android:paddingLeft="30dp"
            />

    </LinearLayout>


    <LinearLayout
        android:orientation="vertical"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        >

        <TableLayout
            android:orientation="vertical"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_gravity="center"
            android:stretchColumns="*"
            android:layout_centerHorizontal="true"
            android:paddingLeft="0dp"
            android:paddingRight="2dp"
            android:layout_marginTop="20dp"
            android:layout_marginLeft="10dp"
            android:layout_marginRight="10dp"
            android:showDividers="none"
            android:background="#ffffff"
            android:paddingBottom="2dp">


            <TableRow
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:padding="0dp"
                android:layout_margin="0dp"
                android:paddingTop="5dp"
                android:paddingBottom="5dp">

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text=""
                    android:textColor="@color/primaryColor"
                    android:textStyle="bold"
                    android:layout_weight="1"
                    android:background="#ffffff"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="OPEN"
                    android:textColor="@color/primaryColor"
                    android:textStyle="bold"
                    android:layout_weight="2"
                    android:textSize="16dp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:paddingTop="7dp"
                    android:paddingBottom="5dp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="CLOSE"
                    android:textColor="@color/primaryColor"
                    android:textStyle="bold"
                    android:layout_weight="2"
                    android:textSize="16dp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:paddingTop="7dp"
                    android:paddingBottom="5dp" />

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="wrap_content"
                    android:text="Closed All Day"
                    android:textColor="@color/primaryColor"
                    android:textStyle="bold"
                    android:layout_weight="2"
                    android:textSize="14dp"
                    android:gravity="center"
                    android:background="#ffffff"
                    android:paddingTop="7dp"
                    android:paddingBottom="5dp" />



            </TableRow>

<!--MONDAY-->

            <TableRow
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_gravity="center_horizontal"
                android:padding="0dp"
                android:layout_margin="0dp"
                >

                <TextView
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text="Mon"
                    android:textColor="@color/primaryColor"
                    android:textStyle="bold|normal"
                    android:gravity="center"
                    android:textSize="16dp"
                    android:layout_weight="1"
                    android:background="#ffffff"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp" />


                <EditText
                    android:id="@+id/mon_open"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:textColor="#ffffff"
                    android:textIsSelectable="true"
                    android:layout_weight="2"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:hint="Click to enter"
                    android:textColorHint="@color/primaryColorLight"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp" />

                <EditText
                    android:id="@+id/mon_close"
                    android:layout_width="0dp"
                    android:layout_height="match_parent"
                    android:text=""
                    android:textColor="#ffffff"
                    android:textIsSelectable="true"
                    android:layout_weight="2"
                    android:background="@drawable/cell_shape"
                    android:gravity="center"
                    android:textSize="16dp"
                    android:textStyle="bold"
                    android:hint="Click to enter"
                    android:textColorHint="@color/primaryColorLight"
                    android:paddingTop="5dp"
                    android:paddingBottom="5dp" />

                <CheckBox
                    android:layout_width="0dp"
                    android:layout_height="40dp"
                    android:layout_weight="1.5"
                    android:text="Yes"
                    android:id="@+id/rb_mon"
                    android:textColor="#ffffff"
                    android:textStyle="bold"
                    android:checked="false"
                    android:background="@drawable/cell_shape"
                    android:textSize="16dp"
                    android:clickable = "true"  />

            </TableRow>
<!--TUESDAY-->
            <TableRow
                android:orientation="horizontal"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
